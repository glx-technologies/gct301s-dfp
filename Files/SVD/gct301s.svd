  <cpu>                                                           <!-- details about the cpu embedded in the device -->
    <name>CM0</name>
    <revision>r0p0</revision>
    <endian>little</endian>
    <mpuPresent>false</mpuPresent>
    <fpuPresent>false</fpuPresent>
    <nvicPrioBits>2</nvicPrioBits>
    <vendorSystickConfig>false</vendorSystickConfig>
  </cpu>
  <addressUnitBits>8</addressUnitBits>                            <!-- byte addressable memory -->
  <width>32</width>                                               <!-- bus width is 32 bits -->
  <!-- default settings implicitly inherited by subsequent sections -->
  <size>32</size>                                                 <!-- this is the default size (number of bits) of all peripherals
                                                                       and register that do not define "size" themselves -->
  <access>read-write</access>                                     <!-- default access permission for all subsequent registers -->
  <resetValue>0x00000000</resetValue>                             <!-- by default all bits of the registers are initialized to 0 on reset -->
  <resetMask>0xFFFFFFFF</resetMask>                               <!-- by default all 32Bits of the registers are used -->

  <peripherals>
    
    <!-- SYSCTRL -->
    <peripheral>
      <name>SYSCTRL</name>
      <version>1.0</version>
      <description>.</description>
      <groupName>SYSCTRL</groupName>
      <baseAddress>0x4001D000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <interrupt>
        <name>EXT0</name>
        <value>17</value>
        <description>GPIO 2 interrupt</description>
      </interrupt>

      <interrupt>
        <name>EXT1</name>
        <value>18</value>
        <description>GPIO 3 interrupt</description>
      </interrupt>
      
      <interrupt>
        <name>EXT2</name>
        <value>19</value>
        <description>GPIO 4 interrupt</description>
      </interrupt>
      
      <interrupt>
        <name>EXT3</name>
        <value>20</value>
        <description>GPIO 5 interrupt</description>
      </interrupt>

      <interrupt>
        <name>IRQ26</name>
        <value>26</value>
      </interrupt>

      <interrupt>
        <name>IRQ27</name>
        <value>27</value>
      </interrupt>

      <interrupt>
        <name>IRQ28</name>
        <value>28</value>
      </interrupt>

      <interrupt>
        <name>IRQ29</name>
        <value>29</value>
      </interrupt>

      <interrupt>
        <name>IRQ30</name>
        <value>30</value>
      </interrupt>

      <interrupt>
        <name>IRQ31</name>
        <value>31</value>
      </interrupt>


      <registers>
        <!-- SYS_RST_STA -->
        <register>
          <name>SYS_RST_STA</name>
          <description>System reset status</description>
          <addressOffset>0x0</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x0</resetValue>

          <fields>
            <!-- WDT_TIMEOUT -->
            <field>
              <name>WDT_TIMEOUT</name>
              <description>Watchdog timeout</description>
              <bitRange>[0:0]</bitRange>
            </field>  

            <!-- CM0LOCKUP -->
            <field>
              <name>CMOLOCKUP</name>
              <description>Core lockup</description>
              <bitRange>[1:1]</bitRange>
            </field>
          </fields>
        </register>

        <!-- SYS_CLK_STA -->
        <register>
          <name>SYS_CLK_STA</name>
          <description>System clock status</description>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x0</resetValue>

          <fields>
            <!-- DPLL_LOCK -->
            <field>
              <name>DPLL_LOCK</name>
              <description>DPLL lock status</description>
              <bitRange>[0:0]</bitRange>
            </field>  
          </fields>
        </register>

        <!-- SWD_ERA_KEY -->
        <register>
          <name>SWD_ERA_KEY</name>
          <description>SWD erase key (0xC3A1)</description>
          <addressOffset>0x08</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x0</resetValue>

          <fields>
            <!-- ERA_KEY -->
            <field>
              <name>ERA_KEY</name>
              <description>ERA_KEY</description>
              <bitRange>[31:0]</bitRange>
            </field>  
          </fields>
        </register>
        
        <!-- SWD_ERA_CMD -->
        <register>
          <name>SWD_ERA_CMD</name>
          <description>SWD erase command</description>
          <addressOffset>0x0C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x0</resetValue>

          <fields>
            <!-- CFERA -->
            <field>
              <name>CFERA</name>
              <description>CFERA</description>
              <bitRange>[0:0]</bitRange>
            </field>  
            
            <!-- DFERA -->
            <field>
              <name>DFERA</name>
              <description>DFERA</description>
              <bitRange>[1:1]</bitRange>
            </field>  
          </fields>
        </register>

        <!-- RST_CR -->
        <register>
          <name>RST_CR</name>
          <description>Reset configuration</description>
          <addressOffset>0x10</addressOffset>
          <fields>
            <field> <name>RSTN_KEY</name>     <bitRange>[7:0]</bitRange>  </field>
          </fields>
        </register>

        <!-- CLK_DIVSEL -->
        <register>
          <name>CLK_DIVSEL</name>
          <description>.</description>
          <addressOffset>0x14</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x1</resetValue>
          <resetMask>0x0007</resetMask>

          <fields>
            <!-- DIVSEL -->
            <field>
              <name>DIVSEL</name>
              <description>Clock divider selection</description>
              <bitRange>[2:0]</bitRange>
            </field>
          </fields>
        </register>
        
        <!-- CLK_SRCSEL -->
        <register>
          <name>CLK_SRCSEL</name>
          <description>.</description>
          <addressOffset>0x18</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x0</resetValue>
          <resetMask>0x0007</resetMask>

          <fields>
            <!-- LCDCLKSEL -->
            <field>
              <name>LCDCLKSEL</name>
              <description>LCD clock selection</description>
              <bitRange>[0:0]</bitRange>
            </field>
            <!-- LXTALSEL -->
            <field>
              <name>LXTALSEL</name>
              <description>HSCLK/LXTAL selection</description>
              <bitRange>[1:1]</bitRange>
            </field>
            <!-- HXTALSEL -->
            <field>
              <name>HXTALSEL</name>
              <description>HSRC/HXTAL selection</description>
              <bitRange>[2:2]</bitRange>
            </field>
          </fields>
        </register>

        <!-- NMI_CTL -->
        <register>
          <name>NMI_CTL</name>
          <description>NMI control</description>
          <addressOffset>0x1C</addressOffset>
          <fields>
            <field> <name>NMISEL</name>     <bitRange>[4:0]</bitRange>  </field>
            <field> <name>NMIEN</name>      <bitRange>[5:5]</bitRange>  </field>
          </fields>
        </register>

        <!-- HSRC_CTL -->
        <register>
          <name>HSRC_CTL</name>
          <description>HSRC control</description>
          <addressOffset>0x20</addressOffset>
          <fields>
            <field> <name>PWRDN</name>     <bitRange>[1:1]</bitRange>  </field>
            <field> <name>HTRIM</name>     <bitRange>[6:2]</bitRange>  </field>
            <field> <name>HTRIM_EN</name>  <bitRange>[7:7]</bitRange>  </field>
          </fields>
        </register>
        
        <!-- LSRC_CTL -->
        <register>
          <name>LSRC_CTL</name>
          <description>LSRC control</description>
          <addressOffset>0x24</addressOffset>
          <resetValue>0x20</resetValue>
          <fields>
            <field> <name>LSRC_ENB</name>     <bitRange>[0:0]</bitRange>  </field>
            <field> <name>VREF_ENB</name>     <bitRange>[1:1]</bitRange>  </field>
            <field> <name>LTRIM</name>        <bitRange>[5:2]</bitRange>  </field>
            <field> <name>LTRIM_EN</name>     <bitRange>[6:6]</bitRange>  </field>
          </fields>
        </register>

        <!-- PWR_CTL -->
        <register>
          <name>PWR_CTL</name>
          <description>Power control</description>
          <addressOffset>0x28</addressOffset>
          <resetValue>0x0</resetValue>
          <fields>
            <field> <name>VRACTIVE</name>     <bitRange>[0:0]</bitRange>  </field>
            <field> <name>SRAMPS_EN</name>    <bitRange>[1:1]</bitRange>  </field>
            <field> <name>ADCPS_EN</name>     <bitRange>[2:2]</bitRange>  </field>
          </fields>
        </register>

        <!-- DPLL_CTL -->
        <register>
          <name>DPLL_CTL</name>
          <description>DPLL control</description>
          <addressOffset>0x2C</addressOffset>
          <resetValue>0x0</resetValue>
          <fields>
            <field> <name>DPLL_EN</name>     <bitRange>[0:0]</bitRange>  </field>
          </fields>
        </register>
        
        <!-- CHIP_CFG -->
        <register>
          <name>CHIP_CFG</name>
          <description>Chip configuration</description>
          <addressOffset>0x30</addressOffset>
          <resetValue>0x0</resetValue>
          <access>read-only</access>
          <fields>
            <field> <name>LXTAL_CFG</name>     <bitRange>[1:0]</bitRange>    </field>
            <field> <name>BOND_A_SEL</name>    <bitRange>[2:2]</bitRange>    </field>
            <field> <name>BOND_B_SEL</name>    <bitRange>[3:3]</bitRange>    </field>
            <field> <name>OSC32K_ADJ</name>    <bitRange>[6:4]</bitRange>    </field>
            <field> <name>LSRC_TRIM</name>     <bitRange>[10:7]</bitRange>   </field>
            <field> <name>HSRC_TRIM</name>     <bitRange>[15:11]</bitRange>  </field>
          </fields>
        </register>
      
        <!-- SYS_CPL -->
        <register>
          <name>SYS_CPL</name>
          <description>System code protection</description>
          <addressOffset>0x34</addressOffset>
          <resetValue>0x0</resetValue>
          <access>read-only</access>
          <fields>
            <field> <name>CPL</name>     <bitRange>[1:0]</bitRange>  </field>
          </fields>
        </register>
        
        <!-- HXTAL_CTL -->
        <register>
          <name>HXTAL_CTL</name>
          <description>HXTAL control</description>
          <addressOffset>0x38</addressOffset>
          <resetValue>0x0</resetValue>
          <fields>
            <field> <name>HXTAL_EN</name>     <bitRange>[0:0]</bitRange>  </field>
          </fields>
        </register>

        <!-- RTC_CTL -->
        <register>
          <name>RTC_CTL</name>
          <description>RTC control</description>
          <addressOffset>0x3C</addressOffset>
          <resetValue>0x0</resetValue>
          <fields>
            <field> <name>IGN_RSTN</name>        <bitRange>[0:0]</bitRange>  </field>
            <field> <name>SW_RESET_RTC</name>    <bitRange>[1:1]</bitRange>  </field>
            <field> <name>PD_LXTAL</name>        <bitRange>[2:2]</bitRange>  </field>
          </fields>
        </register>
        
        <!-- WDT_CTL -->
        <register>
          <name>WDT_CTL</name>
          <description>Watchdog trigger control</description>
          <addressOffset>0x40</addressOffset>
          <resetValue>0x0</resetValue>
          <fields>
            <field> <name>WDT_TRIG</name>     <bitRange>[0:0]</bitRange>  </field>
          </fields>
        </register>
      </registers>
    </peripheral>

    <!-- WDT -->
    <peripheral>
      <name>WDT</name>
      <description>Watchdog</description>
      <baseAddress>0x40006000</baseAddress>
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>
      
      <interrupt>
        <name>WDT</name>
        <value>1</value>
      </interrupt>

      <registers>
        <register>
          <name>CFG</name>
          <description>Watchdog configuration</description>
          <addressOffset>0x0</addressOffset>
          <fields>
            <field> <name>ON</name>       <bitRange>[0:0]</bitRange> <access>read-only</access> </field>
            <field> <name>LOAD_SEL</name> <bitRange>[3:1]</bitRange> <access>read-only</access> </field>
            <field> <name>WARN_EN</name>  <bitRange>[4:4]</bitRange> <access>read-only</access> </field>
            <field> <name>LPM_EN</name>   <bitRange>[5:5]</bitRange> <access>read-only</access> </field>
            <field> <name>DBG_HALT</name> <bitRange>[6:6]</bitRange> <access>read-only</access> </field>
          </fields>
        </register>

        <register>
          <name>IF</name>
          <description>Interrupt flag</description>
          <addressOffset>0x4</addressOffset>
          <fields>
            <field> <name>WARN_IF</name> <bitRange>[0:0]</bitRange> </field>
          </fields>
        </register>

        <register>
          <name>FEED</name>
          <description>Watchdog feed command, write 0xBB66AA55 to feed.</description>
          <addressOffset>0x8</addressOffset>
          <access>write-only</access>
		  <resetValue>0xBB66AA55</resetValue>
        </register>
        
        <register>
          <name>LOAD</name>
          <description>Watchdog timeout reload selection.</description>
          <addressOffset>0x0C</addressOffset>
          <access>read-only</access>
        </register>

        <register>
          <name>COUNT</name>
          <description>Watchdog latched count value.</description>
          <addressOffset>0x10</addressOffset>
        </register>

        <register>
          <name>LATCH</name>
          <description>Update COUNT register.</description>
          <addressOffset>0x14</addressOffset>
		  <fields>
            <field> <name>LATCH_EN</name>     <bitRange>[0:0]</bitRange>     </field>
          </fields>
        </register>
      </registers>
    </peripheral>
    
    <!-- RTC -->
    <peripheral>
      <name>RTC</name>
      <version>1.0</version>
      <description>RTC</description>
      <groupName>RTC</groupName>
      <baseAddress>0x40000000</baseAddress>
      <size>32</size>
      <access>read-write</access>

      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>

      <interrupt>
        <name>RTC</name>
        <value>2</value>
      </interrupt>

      <registers>
        <!-- CLOCK -->
        <register>
          <name>CLOCK</name>
          <description>Clock</description>
          <addressOffset>0x0</addressOffset>
          <fields>
            <field> <name>SEC</name> <bitRange>[6:0]</bitRange>   </field>
            <field> <name>MIN</name> <bitRange>[14:8]</bitRange>  </field>
            <field> <name>HOUR</name>   <bitRange>[21:16]</bitRange> </field>
          </fields>
        </register>
      
        <!-- CALENDAR -->
        <register>
          <name>CALENDAR</name>
          <description>Calendar</description>
          <addressOffset>0x04</addressOffset>
          <fields>
            <field> <name>DAY</name>     <bitRange>[5:0]</bitRange>     </field>
            <field> <name>MONTH</name>   <bitRange>[12:8]</bitRange>    </field>
            <field> <name>YEAR</name>    <bitRange>[23:16]</bitRange>   </field>
            <field> <name>WEEK</name>    <bitRange>[26:24]</bitRange>   </field>
          </fields>
        </register>

        <!-- STATUS -->
        <register>
          <name>STATUS</name>
          <description>Status</description>
          <addressOffset>0x08</addressOffset>
          <access>read-only</access>
          <fields>
            <field> <name>VALIDITY</name>     <bitRange>[0:0]</bitRange>     </field>
          </fields>
        </register>

        <!-- LOAD -->
        <register>
          <name>LOAD</name>
          <description>Load</description>
          <addressOffset>0x0C</addressOffset>
          <fields>
            <field> <name>LOAD_EN</name>     <bitRange>[0:0]</bitRange>     </field>
          </fields>
        </register>

        <!-- LATCH -->
        <register>
          <name>LATCH</name>
          <description>Latch</description>
          <addressOffset>0x10</addressOffset>
          <fields>
            <field> <name>LATCH_EN</name>     <bitRange>[0:0]</bitRange>     </field>
          </fields>
        </register>

        <!-- ACSEL -->
        <register>
          <name>ACSEL</name>
          <description>Adjust cycle selection</description>
          <addressOffset>0x14</addressOffset>
          <fields>
            <field> <name>AC</name>     <bitRange>[1:0]</bitRange>     </field>
          </fields>
        </register>

        <!-- ADJUST -->
        <register>
          <name>ADJUST</name>
          <description>Adjustment amount</description>
          <addressOffset>0x18</addressOffset>
          <fields>
            <field> <name>ADJ</name>     <bitRange>[8:0]</bitRange>     </field>
          </fields>
        </register>
        
        <!-- ALARMCTL -->
        <register>
          <name>ALARMCTL</name>
          <description>Alarm control</description>
          <addressOffset>0x1C</addressOffset>
          <fields>
            <field> <name>AM</name>     <bitRange>[3:0]</bitRange>     </field>
            <field> <name>DW</name>     <bitRange>[4:4]</bitRange>     </field>
          </fields>
        </register>

        <!-- ALARM -->
        <register>
          <name>ALARM</name>
          <description>Alarm</description>
          <addressOffset>0x20</addressOffset>
          <fields>
            <field> <name>SEC</name>      <bitRange>[6:0]</bitRange>     </field>
            <field> <name>MIN</name>      <bitRange>[14:8]</bitRange>    </field>
            <field> <name>HOUR</name>     <bitRange>[21:16]</bitRange>   </field>
            <field> <name>DAY</name>      <bitRange>[30:24]</bitRange>   </field>
          </fields>
        </register>
        
        <!-- IE -->
        <register>
          <name>IE</name>
          <description>Interrupt enable</description>
          <addressOffset>0x24</addressOffset>
          <fields>
            <field> <name>SEC</name>     <bitRange>[0:0]</bitRange>     </field>
            <field> <name>MIN</name>     <bitRange>[1:1]</bitRange>     </field>
            <field> <name>HOUR</name>    <bitRange>[2:2]</bitRange>     </field>
            <field> <name>DAY</name>     <bitRange>[3:3]</bitRange>     </field>
            <field> <name>ALM</name>     <bitRange>[4:4]</bitRange>     </field>
            <field> <name>TM0</name>     <bitRange>[5:5]</bitRange>     </field>
            <field> <name>TM1</name>     <bitRange>[6:6]</bitRange>     </field>
            <field> <name>XSTP</name>    <bitRange>[7:7]</bitRange>     </field>
            <field> <name>ADJ</name>     <bitRange>[8:8]</bitRange>     </field>
          </fields>
        </register>

        <!-- IF -->
        <register>
          <name>IF</name>
          <description>Interrupt flag</description>
          <addressOffset>0x28</addressOffset>
          <fields>
            <field> <name>SEC</name>     <bitRange>[0:0]</bitRange>     </field>
            <field> <name>MIN</name>     <bitRange>[1:1]</bitRange>     </field>
            <field> <name>HOUR</name>    <bitRange>[2:2]</bitRange>     </field>
            <field> <name>DAY</name>     <bitRange>[3:3]</bitRange>     </field>
            <field> <name>ALM</name>     <bitRange>[4:4]</bitRange>     </field>
            <field> <name>TM0</name>     <bitRange>[5:5]</bitRange>     </field>
            <field> <name>TM1</name>     <bitRange>[6:6]</bitRange>     </field>
            <field> <name>XSTP</name>    <bitRange>[7:7]</bitRange>     </field>
            <field> <name>ADJ</name>     <bitRange>[8:8]</bitRange>     </field>
          </fields>
        </register>

        <!-- CTL -->
        <register>
          <name>CTL</name>
          <description>Control</description>
          <addressOffset>0x30</addressOffset>
          <fields>
            <field> <name>TM0EN</name>     <bitRange>[0:0]</bitRange>     </field>
            <field> <name>TM1EN</name>     <bitRange>[1:1]</bitRange>     </field>
            <field> <name>ALMEN</name>     <bitRange>[2:2]</bitRange>     </field>
          </fields>
        </register>

        <!-- TMR0 -->
        <register>
          <name>TMR0</name>
          <description>Timer 0 initial value</description>
          <addressOffset>0x34</addressOffset>
          <fields>
            <field> <name>INI</name>     <bitRange>[7:0]</bitRange>     </field>
          </fields>
        </register>

        <!-- TMR1 -->
        <register>
          <name>TMR1</name>
          <description>Timer 1 initial value</description>
          <addressOffset>0x38</addressOffset>
          <fields>
            <field> <name>INI</name>     <bitRange>[7:0]</bitRange>     </field>
          </fields>
        </register>

        <!-- TMRSEL -->
        <register>
          <name>TMRSEL</name>
          <description>Timer polarity</description>
          <addressOffset>0x3C</addressOffset>
          <fields>
            <field> <name>TMR0POL</name>     <bitRange>[0:0]</bitRange>     </field>
